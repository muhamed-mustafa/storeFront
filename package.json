{
  "name": "storefront",
  "version": "0.1.0",
  "description": "StoreFront BackEnd API",
  "main": "index.ts",
  "scripts": {
    "start": "nodemon src/index.ts",
        "build": "npx tsc",
        "tsc": "tsc",
        "test": "npm run create:test:db && db-migrate --env test up && npm run jasmine || npm run drop:test:db",
        "jasmine": "cross-env ENV=test jasmine-ts --config jasmine.json",
        "create:dev:db": "db-migrate db:create storefront --config database.json -e conn && npm run db:up",
        "create:test:db": "db-migrate db:create storefront_test --config database.json -e conn",
        "drop:test:db": "db-migrate db:drop storefront_test",
        "db:up": "db-migrate up",
        "db:down": "db-migrate down",
        "format": "prettier --write src//*.ts",
        "lint": "eslint src//*.ts",
        "lint:fix": "eslint src/**/*.ts --fix"
  },
  "author": "Muhammed Mustafa",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.8",
    "@types/pg": "^8.6.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^3.99.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4"
  }
}
